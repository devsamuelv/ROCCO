{
	"configurations": [
		{
			"browse": {
				"databaseFilename": "${default}",
				"limitSymbolsToIncludedHeaders": true
			},
			"compileCommands": "${workspaceFolder}/build/compile_commands.json",
			"includePath": [
				"/opt/ros/rolling/include/**",
				"/usr/include/**",
				"/usr/local/include/**"
			],
			"name": "ROS",
			"intelliSenseMode": "linux-clang-x64",
			"compilerPath": "/usr/bin/clang",
			"cStandard": "c11",
			"cppStandard": "c++17"
		}
	],
	"version": 4
}
